{
  "name": "capturoo-express-app",
  "version": "0.15.0",
  "description": "Capturoo API built on Node.js / Express",
  "main": "lib/app.js",
  "scripts": {
    "coverage": "google-chrome ./coverage/index.html",
    "start": "nodemon --inspect server.js",
    "test": "env TEST_EMAIL=andy@andyfusniak.com TEST_PASSWORD=testtest TEST_SHOWTOKEN=true nyc --all --clean --reporter=html mocha --slow 3000",
    "test-staging": "env TEST_EMAIL=andy@andyfusniak.com TEST_PASSWORD=testtest TEST_ENDPOINT=https://api-staging.capturoo.com mocha --slow 1000"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:andyfusniakteam/capturoo-express-app.git"
  },
  "nyc": {
    "include": [
      "**"
    ],
    "exclude": [
      "index.js",
      "config.js",
      "webpack.config.js",
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/*_test.js",
      "**/__tests__/**",
      "**/node_modules/**"
    ]
  },
  "author": "Andy Fusniak",
  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jsonschema": "^1.1.6",
    "firebase-admin": "^5.12.1"
  },
  "devDependencies": {
    "capturoo-dashboard-sdk": "file:../capturoo-dashboard-sdk",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^11.8.0",
    "supertest": "^3.1.0"
  }
}
